<%= simple_form_for(@market) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :group %>
    <%= f.input :animal %>
    <%= f.input :owner %>
    <%= f.input :status %>
    <%= f.input :bids %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
