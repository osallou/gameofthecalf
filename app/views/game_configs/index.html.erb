<h1>Listing game_configs</h1>

<table>
  <tr>
    <th>Default</th>
    <th>Nbtrait</th>
    <th>Mortality</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @game_configs.each do |game_config| %>
  <tr>
    <td><%= game_config.default %></td>
    <td><%= game_config.nbtrait %></td>
    <td><%= game_config.mortality %></td>
    <td><%= link_to 'Show', game_config %></td>
    <td><%= link_to 'Edit', edit_game_config_path(game_config) %></td>
    <td><%= link_to 'Destroy', game_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Game config', new_game_config_path %>
